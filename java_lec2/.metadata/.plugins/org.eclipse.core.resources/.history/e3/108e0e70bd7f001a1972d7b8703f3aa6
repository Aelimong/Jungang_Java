import java.util.*;

public class MapEx {

	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<String, Integer>();
		map.put("blue",96);
		map.put("hong",86);
		map.put("white",92);
		
		String name=null; //최고점수 받은 아이디
		int maxScore=0;   //최고점수
		int totalScore=0; //점수합계
		
		System.out.println(map.get("blue"));
		
		
		
	}

}
